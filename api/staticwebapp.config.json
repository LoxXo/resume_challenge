{
    "trailingSlash": "auto",
    "platform": {
    "apiRuntime": "python:3.10"
    },
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET"]
      },
      {
        "route": "/api/*",
        "methods": ["PUT", "POST", "PATCH", "DELETE"],
        "allowedRoles": ["administrator"]
      }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
      }
    }